@model ResultAboutDto
@{
    Layout = null;
}
<section class="w3l-index3" id="about">
    <div class="midd-w3 pb-5">
        <div class="container pb-lg-5 pb-md-4 pb-2">
            <div class="row">
                <div class="col-lg-5 pr-lg-0">
                    <div class="w3l-left-img">
                    </div>
                </div>
                <div class="col-lg-7 pl-lg-0">
                    <div class="w3l-right-info">
                        <h6 class="title-small">Bİz Kİmİz ?</h6>
                        <h3 class="title-big">Yurt İçi Ve Yurt Dışı Tecrübe</h3>
                        <p class="mt-4">
                           @Model.AboutDesc1
                        </p>
                        <p class="mt-3">
                                @Model.AboutDesc2
                        </p>
                        <ul class="w3l-right-book w3l-right-book-grid mt-md-5 mt-4">
                                <li><span class="fa fa-check" aria-hidden="true"></span>@Model.AboutService1</li>
                                <li><span class="fa fa-check" aria-hidden="true"></span>@Model.AboutService2</li>
                                <li><span class="fa fa-check" aria-hidden="true"></span>@Model.AboutService3</li>
                                <li><span class="fa fa-check" aria-hidden="true"></span>@Model.AboutService4</li>
                                <li><span class="fa fa-check" aria-hidden="true"></span>@Model.AboutService5</li>
                                <li><span class="fa fa-check" aria-hidden="true"></span>@Model.AboutService6</li>
                        </ul>
                        <a href="#services" class="btn btn-style btn-primary mt-4">Hizmetlerimizi keşfedin</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>